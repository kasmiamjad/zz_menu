{
    "cleanUrls": true,
    "trailingSlash": false,
    "routes": [
        {
            "src": "/index.js",
            "dest": "/script.js"
        },
        {
            "src": "/style.css",
            "dest": "/style.css"
        },
        {
            "src": "/menu-data.json",
            "dest": "/menu-data.json"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}